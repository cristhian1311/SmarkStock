<odoo>
    <record id="view_product_register_form" model="ir.ui.view">
        <field name="name">product.register.form</field>
        <field name="model">product.register</field>
        <field name="arch" type="xml">
            <form string="Producto">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="description"/>
                        <field name="price"/>
                        <field name="quantity"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="view_product_register_tree" model="ir.ui.view">
        <field name="name">product.register.tree</field>
        <field name="model">product.register</field>
        <field name="arch" type="xml">
            <tree string="Productos">
                <field name="name"/>
                <field name="code"/>
                <field name="price"/>
                <field name="quantity"/>
            </tree>
        </field>
    </record>


    <record id="action_product_register" model="ir.actions.act_window">
        <field name="name">Productos</field>
        <field name="res_model">product.register</field>
        <field name="view_mode">tree,form</field>
    </record>


    <menuitem id="menu_productos_root" 
              name="Productos"
              web_icon="fa fa-cube"
              sequence="5"/>


    <menuitem id="menu_product_register" 
              name="Registro de Productos"
              parent="menu_productos_root"
              action="action_product_register"
              sequence="10"/>
</odoo>